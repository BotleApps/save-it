{
  "name": "save-it",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "Save and organize your links, articles, and resources",
  "author": "BotleApps",
  "scripts": {
    "start": "EXPO_ROUTER_APP_ROOT=app expo start --tunnel",
    "start-web": "expo start --web --tunnel",
    "start-web-dev": "DEBUG=expo* expo start --web --tunnel",
    "build": "expo export -p web",
    "vercel-build": "npx expo export --platform web --output-dir dist && cp -r public/* dist/",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:all": "eas build --platform all --profile production",
    "build:preview": "eas build --platform all --profile preview",
    "build:local:android": "./build-android-local.sh",
    "build:local:android-store": "./build-android-store.sh",
    "build:local:ios": "./build-ios-local.sh",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "update": "eas update --branch production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.25",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.8",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "^0.32.15",
    "expo-router": "~5.1.10",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "^13.16.0",
    "zustand": "^5.0.4"
    ,"@sentry/expo": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@expo/ngrok": "^4.1.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-require-context": "^0.1.1",
    "babel-preset-expo": "~13.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.13",
    "ts-jest": "^29.3.4",
    "typescript": "~5.8.3"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  }
}
